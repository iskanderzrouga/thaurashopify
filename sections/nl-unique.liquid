{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top_m }}px;
    padding-bottom: {{ section.settings.padding_bottom_m }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

<div
  id="{{ section.settings.section_id }}"
  class="section-{{ section.id }}-padding px-5 relative"
  style="background-color: {{ section.settings.bgcolor }}"
>
  {% if section.settings.bg != blank %}
    {{
      section.settings.bg
      | image_url: width: 1920
      | image_tag: class: 'w-full absolute top-0 left-0 h-full object-cover', sizes: '1920px'
    }}
  {% endif %}

  <div
    class="mx-auto w-full max-w-[1420px] flex flex-col items-center justify-center gap-8 relative"
    style="max-width: {{ section.settings.max_width }}px;"
  >
    <div
      class="flex flex-col items-center justify-center gap-10 text-center mx-auto relative w-full"
    >
      {% if section.settings.show_section1 %}
        <div
          class="relative px-5 py-6 md:p-10 w-full border border-[#AAAAAA] rounded-[16px]"
          style="background: linear-gradient(to bottom, #FFFFFF, #D0D0D0);"
        >
          {% if section.settings.s1_bg != blank %}
            {{
              section.settings.s1_bg
              | image_url: width: 1140
              | image_tag:
                class: 'w-full absolute top-0 left-0 h-full object-cover border border-[#F6F6F6] rounded-[16px]',
                sizes: '1140px'
            }}
          {% endif %}

          <div class="relative mx-auto grid grid-cols-1 md:grid-cols-2 gap-5 md:gap-10 items-center">
            {% if section.settings.s1_image != blank %}
              {{ section.settings.s1_image | image_url: width: 300 | image_tag: class: 'w-full max-w-[300px] mx-auto' }}
            {% endif %}

            <div class="flex flex-col gap-4">
              {% if section.settings.s1_heading != blank %}
                <h2
                  class="text-[18px] md:text-[22px] font-bold !leading-tight text-[#252726] text-center md:text-left"
                >
                  {{ section.settings.s1_heading }}
                </h2>
              {% endif %}

              <div class="w-full">
                {% for block in section.blocks %}
                  {% if block.type == 's1_items' %}
                    <div class="w-full flex items-center gap-2">
                      {% if block.settings.icon != blank %}
                        {{ block.settings.icon | image_url: width: 16 | image_tag: class: 'w-full max-w-[16px]' }}
                      {% endif %}
                      {% if block.settings.description != blank %}
                        <div
                          class="text-[17px] md:text-[17px] font-normal !leading-[1.5] text-[#252726]"
                        >
                          {{ block.settings.description }}
                        </div>
                      {% endif %}
                    </div>
                  {% endif %}
                {% endfor %}
              </div>

              {% if section.settings.s1_description != blank %}
                <div
                  class="text-[10px] md:text-[10px] font-normal !leading-[1.5] text-[#252726] text-center md:text-left"
                >
                  {{ section.settings.s1_description }}
                </div>
              {% endif %}
            </div>
          </div>
        </div>
      {% endif %}

      <div
        class="relative px-5 py-6 md:p-10 w-full border border-[#AAAAAA] rounded-[16px]"
        style="background: linear-gradient(to bottom, #FFFFFF, #D0D0D0);"
      >
        {% if section.settings.s2_bg != blank %}
          {{
            section.settings.s2_bg
            | image_url: width: 1140
            | image_tag:
              class: 'w-full absolute top-0 left-0 h-full object-cover border border-[#F6F6F6] rounded-[16px]',
              sizes: '1140px'
          }}
        {% endif %}

        <div class="relative flex flex-col w-full gap-[30px] md:gap-10">
          {% if section.settings.s2_heading != blank %}
            <h2
              class="text-[26px] md:text-[36px] font-bold !leading-tight text-[#00754A] text-center"
            >
              {{ section.settings.s2_heading }}
            </h2>
          {% endif %}

          {% if section.settings.s2_subheading != blank %}
            <div
              class="text-[17px] md:text-[20px] font-medium !leading-[1.5] text-[#252726] text-center"
            >
              {{ section.settings.s2_subheading }}
            </div>
          {% endif %}

          <div class="relative mx-auto flex flex-col md:grid md:grid-cols-1 gap-5 md:gap-10 items-center">
            {% if section.settings.s2_image != blank %}
              {{ section.settings.s2_image | image_url: width: 780 | image_tag: class: 'w-full max-w-[780px] mx-auto' }}
            {% endif %}

            <div class="">
              {% if section.settings.s2_description != blank %}
                <div
                  class="text-[17px] md:text-[18px] font-normal !leading-[1.5] text-[#252726] text-center"
                >
                  {{ section.settings.s2_description }}
                </div>
              {% endif %}
            </div>
          </div>

          <div class="grid grid-cols-1 md:grid-cols-3 gap-[35px] md:gap-5 mt-8">
            {% for block in section.blocks %}
              {% if block.type == 's2_items_1' %}
                <div class="relative w-full flex flex-col justify-center text-center gap-2 md:gap-4 px-4 pt-7 pb-4 md:py-12 bg-[#F1E0E036] border border-[#1C5934] rounded-[12px]">
                  {% if block.settings.number != blank %}
                    <div
                      class="absolute left-1/2 transform -translate-x-1/2 -top-[25px] md:-top-[30px] text-[32px] md:text-[40px] font-bold !leading-[1.5] text-[#fff] w-[50px] md:w-[60px] rounded-[60px] flex items-center justify-center bg-[#1C5934]"
                    >
                      {{ block.settings.number }}
                    </div>
                  {% endif %}
                  {% if block.settings.heading != blank %}
                    <div
                      class="text-[17px] md:text-[18px] font-bold !leading-[1.5] text-[#252726]"
                    >
                      {{ block.settings.heading }}
                    </div>
                  {% endif %}
                  {% if block.settings.description != blank %}
                    <div
                      class="text-[17px] md:text-[17px] font-normal !leading-[1.5] text-[#252726]"
                    >
                      {{ block.settings.description }}
                    </div>
                  {% endif %}
                </div>
              {% endif %}
            {% endfor %}
          </div>

          {% if section.settings.s2_subheading2 != blank %}
            <div
              class="text-[18px] md:text-[22px] font-bold !leading-[1.5] text-[#252726] text-center"
            >
              {{ section.settings.s2_subheading2 }}
            </div>
          {% endif %}

          {% if section.settings.s2_description2 != blank %}
            <div
              class="text-[17px] md:text-[18px] font-normal !leading-[1.5] text-[#252726] text-center"
            >
              {{ section.settings.s2_description2 }}
            </div>
          {% endif %}

          {% assign s2_blocks = section.blocks | where: 'type', 's2_items_2' %}
          {% if s2_blocks.size > 0 %}
            <div class="grid grid-cols-4 md:grid-cols-10 gap-[10px] md:gap-5">
              {% for block in section.blocks %}
                {% if block.type == 's2_items_2' %}
                  <div class="s2_items_2 col-span-2 relative w-full flex flex-col items-center text-center gap-4 p-3 md:px-5 md:py-12 bg-[#F1E0E036] border border-[#AAAAAA] rounded-[12px]">
                    {% if block.settings.icon != blank %}
                      {{
                        block.settings.icon
                        | image_url: width: 30
                        | image_tag: class: 'w-full max-w-[24px] md:max-w-[30px]', sizes: '30px'
                      }}
                    {% endif %}

                    {% if block.settings.description != blank %}
                      <div
                        class="text-[14px] md:text-[18px] font-normal !leading-[1.5] text-[#252726]"
                      >
                        {{ block.settings.description }}
                      </div>
                    {% endif %}
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          {% endif %}
        </div>
      </div>

      {% if section.settings.show_section3 %}
        <div
          class="relative px-5 py-6 md:p-10 w-full border border-[#AAAAAA] rounded-[16px]"
          style="background: linear-gradient(to bottom, #FFFFFF, #D0D0D0);"
        >
          {% if section.settings.s3_bg != blank %}
            {{
              section.settings.s3_bg
              | image_url: width: 1140
              | image_tag:
                class: 'w-full absolute top-0 left-0 h-full object-cover border border-[#F6F6F6] rounded-[16px]',
                sizes: '1140px'
            }}
          {% endif %}

          <div class="relative flex flex-col w-full gap-[30px] md:gap-10">
            {% if section.settings.s3_heading != blank %}
              <h2
                class="text-[26px] md:text-[36px] font-bold !leading-tight text-[#00754A] text-center"
              >
                {{ section.settings.s3_heading }}
              </h2>
            {% endif %}

            <div class="grid grid-cols-1 md:grid-cols-3 gap-[10px] md:gap-5">
              {% for block in section.blocks %}
                {% if block.type == 's3_items' %}
                  <div class="relative w-full flex flex-col items-center justify-center text-center p-5 bg-[#F1E0E036] border border-[#AAAAAA] rounded-[12px]">
                    {% if block.settings.description != blank %}
                      <div
                        class="text-[20px] md:text-[24px] font-bold !leading-[1] text-[#252726] w-fit relative"
                      >
                        {{ block.settings.description }}
                        <div class="absolute w-[150%] -left-[25%] top-[10px] bg-white h-[2px] !block"></div>
                      </div>
                    {% endif %}
                  </div>
                {% endif %}
              {% endfor %}
            </div>

            {% if section.settings.s3_subheading != blank %}
              <div
                class="text-[17px] md:text-[20px] font-medium !leading-[1.5] text-[#252726] text-center"
              >
                {{ section.settings.s3_subheading }}
              </div>
            {% endif %}

            {% if section.settings.s3_description != blank %}
              <div
                class="text-[17px] md:text-[18px] font-normal italic p-5 md:p-10 !leading-[1.5] text-[#252726] text-center bg-[#F1E0E036] border border-[#AAAAAA] rounded-[12px]"
              >
                {{ section.settings.s3_description }}
              </div>
            {% endif %}
          </div>
        </div>
      {% endif %}

      <div
        class="relative py-6 md:py-10 w-full border border-[#AAAAAA] rounded-[16px]"
        style="background: linear-gradient(to bottom, #FFFFFF, #D0D0D0);"
      >
        {% if section.settings.s4_bg != blank %}
          {{
            section.settings.s4_bg
            | image_url: width: 1140
            | image_tag:
              class: 'w-full absolute top-0 left-0 h-full object-cover border border-[#F6F6F6] rounded-[16px]',
              sizes: '1140px'
          }}
        {% endif %}

        <div class="relative flex flex-col w-full gap-[30px] md:gap-10">
          {% if section.settings.s4_heading != blank %}
            <h2
              class="text-[26px] md:text-[36px] font-bold !leading-tight text-[#00754A] text-center px-5 md:px-10"
            >
              {{ section.settings.s4_heading }}
            </h2>
          {% endif %}

          <div class="w-full px-5 md:px-10">
            {% for block in section.blocks %}
              {% if block.type == 's4_review' %}
                <div class=" bg-[#F1E0E036] border border-[#AAAAAA] rounded-[16px] w-full grid grid-cols-1 md:grid-cols-2 max-w-[800px] mx-auto">
                  {% if block.settings.image != blank %}
                    {{
                      block.settings.image
                      | image_url: width: 500
                      | image_tag:
                        class: 'w-full h-full object-cover rounded-t-[16px] rounded-bl-[1px] md:rounded-bl-[16px] md:rounded-tr-[1px]',
                        sizes: '500px'
                    }}
                  {% endif %}

                  <div class="h-full flex flex-col justify-center gap-4 items-center md:items-start p-5 md:p-10">
                    {% if block.settings.stars != blank %}
                      {{
                        block.settings.stars
                        | image_url: width: 120
                        | image_tag: class: 'w-full max-w-[120px]', sizes: '120px'
                      }}
                    {% endif %}

                    {% if block.settings.description != blank %}
                      <div class="text-[17px] md:text-[17px] font-normal !leading-[1.5] text-[#252726] text-center md:text-left">
                        {{ block.settings.description }}
                      </div>
                    {% endif %}

                    {% if block.settings.name != blank %}
                      <div class="flex items-center justify-between gap-2 w-full">
                        {% if block.settings.name != blank %}
                          <div class="flex items-center gap-3">
                            {% if block.settings.avatar != blank %}
                              {{
                                block.settings.avatar
                                | image_url: width: 24
                                | image_tag: class: 'w-full max-w-[24px] rounded-[30px]', sizes: '24px'
                              }}
                            {% endif %}
                            <div class="text-[14px] md:text-[16px] font-bold !leading-tight text-[#252726]">
                              {{ block.settings.name }}
                            </div>
                          </div>
                        {% endif %}

                        <div class="flex items-center gap-2">
                          {% if block.settings.icon != blank %}
                            {{
                              block.settings.icon
                              | image_url: width: 14
                              | image_tag: class: 'w-full max-w-[14px]', sizes: '14px'
                            }}
                          {% endif %}
                          {% if block.settings.verified != blank %}
                            <div class="text-[15px] md:text-[15px] font-[300] !leading-tight text-[#252726]">
                              {{ block.settings.verified }}
                            </div>
                          {% endif %}
                        </div>
                      </div>
                    {% endif %}
                  </div>
                </div>
              {% endif %}
            {% endfor %}
          </div>

          <div class="bg-[#1E3A16] py-4 overflow-hidden">
            <div class="nl-unique-s4-animation flex items-center">
              {% for i in (1..10) %}
                <div class="nl-animation__item flex items-center justify-between max-w-[1200px] min-w-[1200px] px-5">
                  {% for block in section.blocks %}
                    {% if block.type == 's4_items' %}
                      <div class="flex items-center justify-center gap-2">
                        {% if block.settings.icon != blank %}
                          {{
                            block.settings.icon
                            | image_url: width: 16
                            | image_tag: class: 'w-full max-w-[16px]', sizes: '16px'
                          }}
                        {% endif %}
                        {% if block.settings.description != blank %}
                          <div
                            class="text-[17px] md:text-[18px] font-bold !leading-[1] text-[#fff]"
                          >
                            {{ block.settings.description }}
                          </div>
                        {% endif %}
                      </div>
                    {% endif %}
                  {% endfor %}
                </div>
              {% endfor %}
            </div>
          </div>

          <style>
            .nl-unique-s4-animation {
              animation: NlUniqueS4Animation 50s linear infinite;
              width: 6000px;
            }
            @keyframes NlUniqueS4Animation {
              0% {
                transform: translateX(0);
              }
              100% {
                transform: translateX(-3000px);
              }
            }
          </style>

          {% comment %}
            <div class="grid grid-cols-2 md:grid-cols-2 gap-[10px] md:gap-5">
              {% for block in section.blocks %}
                {% if block.type == 's4_items' %}
                  <div class="relative w-full flex flex-col md:flex-row items-center md:justify-start gap-5 text-center md:text-left p-5 bg-[#F1E0E036] border border-[#AAAAAA] rounded-[12px]">
                    {% if block.settings.icon != blank %}
                      {{
                        block.settings.icon
                        | image_url: width: 30
                        | image_tag: class: 'w-full max-w-[30px]', sizes: '30px'
                      }}
                    {% endif %}
                    {% if block.settings.description != blank %}
                      <div
                        class="text-[17px] md:text-[22px] font-bold !leading-[1] text-[#252726]"
                      >
                        {{ block.settings.description }}
                      </div>
                    {% endif %}
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          {% endcomment %}

          {% if section.settings.s4_description != blank %}
            <div
              class="text-[17px] md:text-[18px] font-normal !leading-[1.5] text-[#252726] text-center px-5 md:px-10"
            >
              {{ section.settings.s4_description }}
            </div>
          {% endif %}

          {% if section.settings.s4_subheading != blank %}
            <div
              class="text-[24px] md:text-[32px] font-bold !leading-[1.5] text-[#252726] text-center px-5 md:px-10"
            >
              {{ section.settings.s4_subheading }}
            </div>
          {% endif %}

          {% comment %}
            <div class="grid grid-cols-1 md:grid-cols-6 gap-[10px] md:gap-5 px-5 md:px-10">
              {% for block in section.blocks %}
                {% if block.type == 's4_items_2' %}
                  <div class="md:col-span-2 s4_items_2--{{forloop.index}} flex flex-col h-full border border-[#AAAAAA] rounded-[12px]">
                    {% if block.settings.image != blank %}
                      {{
                        block.settings.image
                        | image_url: width: 400
                        | image_tag: class: 'w-full rounded-t-[12px]', sizes: '400px'
                      }}
                    {% endif %}

                    <div class="flex flex-col justify-between text-center flex-1 bg-[#F1E0E036] rounded-b-[12px]">
                      <div class="p-5 flex flex-col {% if block.settings.center_text %} items-center justify-center text-center {% endif %} gap-[14px]">
                        {% if block.settings.heading != blank %}
                          <div
                            class="text-[17px] md:text-[20px] font-bold !leading-tight text-[#252726]"
                          >
                            {{ block.settings.heading }}
                          </div>
                        {% endif %}

                        {% if block.settings.description != blank %}
                          <div
                            class="text-[17px] md:text-[17px] font-normal !leading-[1.5] text-[#252726]"
                          >
                            {{ block.settings.description }}
                          </div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          {% endcomment %}

          {% assign has_s4_items_3 = false %}
          {% for block in section.blocks %}
            {% if block.type == 's4_items_3' %}
              {% assign has_s4_items_3 = true %}
              {% break %}
            {% endif %}
          {% endfor %}
          {% if has_s4_items_3 %}
            <div class="grid grid-cols-1 md:grid-cols-3 gap-[45px] md:gap-5 mt-8 px-5 md:px-10">
              {% for block in section.blocks %}
                {% if block.type == 's4_items_3' %}
                  <div class="relative w-full flex flex-col justify-center text-center gap-4 px-5 pt-10 pb-5 md:py-12 bg-[#F1E0E036] border border-[#1C5934] rounded-[12px]">
                    {% if block.settings.number != blank %}
                      <div
                        class="absolute left-1/2 transform -translate-x-1/2 -top-[25px] md:-top-[30px] text-[32px] md:text-[40px] font-bold !leading-[1.5] text-[#fff] w-[50px] md:w-[60px] rounded-[60px] flex items-center justify-center bg-[#1C5934]"
                      >
                        {{ block.settings.number }}
                      </div>
                    {% endif %}
                    {% if block.settings.heading != blank %}
                      <div
                        class="text-[17px] md:text-[18px] font-bold !leading-[1.5] text-[#252726]"
                      >
                        {{ block.settings.heading }}
                      </div>
                    {% endif %}
                    {% if block.settings.description != blank %}
                      <div
                        class="text-[17px] md:text-[17px] font-normal !leading-[1.5] text-[#252726]"
                      >
                        {{ block.settings.description }}
                      </div>
                    {% endif %}
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          {% endif %}

          {% assign has_s4_items_4 = false %}
          {% for block in section.blocks %}
            {% if block.type == 's4_items_4' %}
              {% assign has_s4_items_4 = true %}
              {% break %}
            {% endif %}
          {% endfor %}
          {% if has_s4_items_4 %}
            <div class="w-full flex flex-col gap-3">
              <div class="grid md:hidden grid-cols-3 gap-2 px-4">
                {% for block in section.blocks %}
                  {% if block.type == 's4_items_4' %}
                    {% if block.settings.btn_text != blank %}
                      <div
                        data-id="{{ section.id }}"
                        data-index="{{ block.settings.index }}"
                        class="{% if block.settings.default %} active {% endif %} nl-unique-s4-btn text-center cursor-pointer border border-[#084C25] rounded-[100px] text-[#084C25] [&.active]:text-white text-[14px] md:text-[20px] font-normal p-2 bg-white [&.active]:bg-[#084C25]"
                      >
                        {{ block.settings.btn_text }}
                      </div>
                    {% endif %}
                  {% endif %}
                {% endfor %}
              </div>

              <div class="grid grid-cols-1 md:grid-cols-3 gap-[45px] md:gap-5 mt-8 px-5 md:px-10 pt-6 md:pt-10">
                {% for block in section.blocks %}
                  {% if block.type == 's4_items_4' %}
                    <div
                      data-id="{{ section.id }}"
                      data-index="{{ block.settings.index }}"
                      class="{% if block.settings.default %} active {% endif %} nl-unique-s4-card [&.active]:flex hidden md:flex relative w-full flex-col text-center gap-4 px-5 pt-10 pb-5 md:py-12 bg-[#F1E0E036] border border-[#1C5934] rounded-[12px]"
                    >
                      {% if block.settings.icon != blank %}
                        {{
                          block.settings.icon
                          | image_url: width: 150
                          | image_tag:
                            class: 'absolute left-1/2 transform -translate-x-1/2 -top-[50px] md:-top-[75px] w-[100px] md:w-[150px] rounded-[150px]',
                            sizes: '150px'
                        }}
                      {% endif %}
                      {% if block.settings.heading != blank %}
                        <div
                          class="text-[20px] md:text-[24px] font-bold !leading-[1.5] text-[#00754A] mt-6 md:mt-12"
                        >
                          {{ block.settings.heading }}
                        </div>
                      {% endif %}
                      {% if block.settings.description != blank %}
                        <div
                          class="text-[17px] md:text-[17px] font-normal !leading-[1.5] text-[#252726] [&>ul]:list-disc [&>ul]:text-left [&>ul]:pl-5"
                        >
                          {{ block.settings.description }}
                        </div>
                      {% endif %}
                    </div>
                  {% endif %}
                {% endfor %}
              </div>
            </div>
          {% endif %}

          {% if section.settings.s4_description_2 != blank %}
            <div
              class="text-[17px] md:text-[18px] font-normal p-5 md:p-10 !leading-[1.5] text-[#252726] text-center px-5 md:px-10"
            >
              {{ section.settings.s4_description_2 }}
            </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>

<script>
  document.addEventListener('DOMContentLoaded', () => {
    const dropdownHeadings = document.querySelectorAll('.section-{{ section.id }}-padding .dropdown-heading');

    dropdownHeadings.forEach((heading) => {
      heading.addEventListener('click', () => {
        const index = heading.getAttribute('data-index');

        // Find parent dropdown-parent with matching data-index
        const parentDropdown = document.querySelector(
          `.section-{{ section.id }}-padding .dropdown-parent[data-index="${index}"]`
        );

        let dropdownIsOpened = false;
        if (parentDropdown && parentDropdown.classList.contains('active')) {
          dropdownIsOpened = true;
        }

        // Remove "active" from all dropdown parents
        document.querySelectorAll('.section-{{ section.id }}-padding .dropdown-parent').forEach((parent) => {
          parent.classList.remove('active');
        });

        // Add "active" to the clicked dropdown parent
        if (parentDropdown) {
          if (dropdownIsOpened) {
            parentDropdown.classList.remove('active');
          } else {
            parentDropdown.classList.add('active');
          }
        }
      });
    });
  });
</script>

<script>
  document.addEventListener('DOMContentLoaded', () => {
    const buttons = document.querySelectorAll('.nl-unique-s4-btn');

    buttons.forEach((btn) => {
      btn.addEventListener('click', () => {
        // Remove 'active' from all buttons
        buttons.forEach((b) => b.classList.remove('active'));
        // Add 'active' to clicked button
        btn.classList.add('active');

        const dataIndex = btn.getAttribute('data-index');
        const dataId = btn.getAttribute('data-id');

        // Find all matching cards with same data-id
        const cards = document.querySelectorAll(`.nl-unique-s4-card[data-id="${dataId}"]`);
        cards.forEach((card) => {
          const cardIndex = card.getAttribute('data-index');
          card.classList.remove('active');
          if (cardIndex === dataIndex) {
            card.classList.add('active');
          }
        });
      });
    });
  });
</script>

